
void main(){
    //Factorial iterativo
    int a,fact;
    //inicializando
    fact = 1;
    a  = 5;
    //mientras

    if(fact == 1){
        fact = 2;
        if(fact  == 2){
            fact =3;
        }
        else{
            fact = 4;
        }
    } else fact = 0;

    while(a == 0){
        fact = fact/a;
        a=a-1;
    }
    
}